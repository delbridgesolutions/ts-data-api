{
  "name": "delbridge-typescript-data-api",
  "version": "0.1.0",
  "description": "Delbridge TypeScript Data API for MongoDB",
  "main": "./src/main.ts",
  "scripts": {
    "build": "npx tsc --project ./tsconfig.json",
    "start": "npx tsc --project ./tsconfig.json && node ./build/main.js",
    "test": "npx jest",
    "test:client": "npx ts-node ./test/client.ts",
    "test:file": "npx jest --coverage=false --verbose"
  },
  "keywords": [
    "mongodb",
    "typescript",
    "data-api",
    "delbridge"
  ],
  "author": "ArturGC",
  "license": "Apache-2.0",
  "dependencies": {
    "h3": "1.15.3",
    "jsonwebtoken": "9.0.2",
    "mongodb": "6.17.0",
    "zod": "3.25.51"
  },
  "devDependencies": {
    "@types/jest": "29.5.14",
    "@types/jsonwebtoken": "9.0.9",
    "@types/node": "22.15.29",
    "axios": "1.9.0",
    "jest": "29.7.0",
    "prettier": "3.5.3",
    "ts-jest": "29.3.4",
    "ts-node": "10.9.2",
    "typescript": "5.8.3"
  },
  "jest": {
    "maxWorkers": 1,
    "preset": "ts-jest",
    "verbose": true
  }
}
